{
    "name": "hostelpass-co/seo-manager",
    "description": "SEO Manager for Laravel Framework",
    "keywords": ["laravel", "seo", "seo-manager", "meta", "meta-data", "open-graph", "og-data"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmytro Krasov",
            "email": "dskrasov@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Krasov\\SeoManager\\SeoManagerServiceProvider"
            ],
            "aliases": {
                "SeoManager": "Krasov\\SeoManager\\Facades\\SeoManager"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Krasov\\SeoManager\\": "src"
        },
        "files":[
            "src/helpers/helpers.php"
        ]
    },
    "require": {
        "php": "^7.1",
        "illuminate/support": "5.5.* || 5.6.* || 5.7.* || 5.8.* || 6.0.*",
        "illuminate/console": "^6.0",
        "illuminate/http": "^6.0",
        "illuminate/view": "^6.0",
        "illuminate/database": "^6.0"
    }
}
